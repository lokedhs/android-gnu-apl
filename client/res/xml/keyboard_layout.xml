<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
          android:keyWidth="12.50%p"
          android:keyHeight="9%p">
    <!--
        NOTE When we add a horizontalGap in pixels, this interferes with keyWidth in percentages adding up to 100%
        NOTE When we have a horizontalGap (on Keyboard level) of 0, this make the horizontalGap (on Key level) to move from after the key to before the key... (I consider this a bug)
     -->
    <!--
        @keylayout:
        4 rows, 6 keys per row

        123 +- <=
        456 */ log
        789 ^e appr
        (0. X  trig

        NOTE you may need additional resource files and strings to use this
     -->
    <Row>
        <Key android:codes="72001" android:keyLabel="`" android:keyWidth="8%p" android:keyEdgeFlags="left"/>
        <Key android:codes="8" android:keyLabel="1" android:keyWidth="8%p"/>
        <Key android:codes="-2" android:keyLabel="2" android:keyWidth="8%p"/>
        <Key android:codes="-1" android:keyLabel="3" android:keyWidth="8%p"/>
        <Key android:codes="33" android:keyLabel="4" android:keyWidth="8%p"/>
        <Key android:codes="34" android:keyLabel="5" android:keyWidth="8%p"/>
        <Key android:codes="35" android:keyLabel="6" android:keyWidth="8%p"/>
        <Key android:codes="36" android:keyLabel="7" android:keyWidth="8%p"/>
        <Key android:codes="37" android:keyLabel="8" android:keyWidth="8%p"/>
        <Key android:codes="38" android:keyLabel="9" android:keyWidth="8%p"/>
        <Key android:codes="39" android:keyLabel="0" android:keyWidth="8%p"/>
        <Key android:codes="45" android:keyLabel="-" android:keyWidth="8%p"/>
        <Key android:codes="46" android:keyLabel="=" android:keyWidth="8%p"/>
    </Row>
